class Solution {
    public int removeElement(int[] nums, int val) {
        int n = nums.length;
        int size = n;
        for(int i = 0 ; i<size; i++)
        {
            if(nums[i] == val)
            {
                int temp = nums[i];
                nums[i] = nums[size-1];
                nums[size-1] = temp;
                size--;
                i=i-1;
            }
        }
        return size;
    }
}
