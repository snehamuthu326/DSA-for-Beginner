class Solution {
    public int removeDuplicates(int[] nums) {
        int n = nums.length;
        int size = n;
        for(int i = 0;i<size -1;i++)
        {
            if(nums[i] == nums[i+1])
            {
                for(int j = i+1; j< size-1;j++)
                {
                    nums[j] = nums[j+1];
                }
                size--;
                i--;
            }
        }
        return size;

    }
}
